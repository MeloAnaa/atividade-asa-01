;
; BIND data file for local loopback interface
;
$TTL 604800
@   IN  SOA ns.asa.br. root.asa.br. (
                  2       ; Serial
             604800       ; Refresh
              86400       ; Retry
            2419200       ; Expire
             604800 )     ; Negative Cache TTL
;
@   IN  NS  ns.
@   IN  A   127.0.0.1
ns  IN  A   127.0.0.1

www IN  A   192.168.1.103
portal IN CNAME www
ana    IN CNAME www



